@model LeandroPea02.Models.UsuarioEntity
@{
    ViewBag.Title = "Crear";
}

<h2>Nuevo usuario</h2>
@using (Html.BeginForm())
{
    @Html.HiddenFor(model => Model.IdUsuario)

    <div class="form-group">
        @Html.LabelFor(model => Model.Nombres)
        @Html.EditorFor(model => Model.Nombres)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => Model.Apellidos)
        @Html.EditorFor(model => Model.Apellidos)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => Model.Usuario)
        @Html.EditorFor(model => Model.Usuario)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => Model.Clave)
        @Html.EditorFor(model => Model.Clave)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => Model.Estado)
        @Html.EditorFor(model => Model.Estado)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => Model.Observacion)
        @Html.EditorFor(model => Model.Observacion)
    </div>

    <div class="form-group">
        <input type="submit" value="Registrar" class="btn btn-primary" />
    </div>
}
